<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="Projekt_Siłownia.UserPage"
             Title="UserPage">
    <VerticalStackLayout>
        <Label Text="User Page"/>
        <Button Text="Wyloguj Się" Clicked="LogOutClicked"/>
        <Picker/>
        <Label Text="Moje Karnety"/>
        <StackLayout Background="AliceBlue">
            <Label x:Name="Rodzaj"/>
            <Label x:Name="Data"/>
            <Button Text="Odnow Karnet" Clicked="OdnowButton_Clicked"/>
        </StackLayout>
    </VerticalStackLayout>
</ContentPage>